{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["theme.config.ts","env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["@intlify/unplugin-vue-i18n/messages","node"]
  }
}
